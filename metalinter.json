{
  "Deadline": "10m",
  "Vendor": true,
  "DisableAll": true,
  "Enable": [
    "deadcode",
    "errcheck",
    "gas",
    "goconst",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "ineffassign",
    "interfacer",
    "megacheck",
    "misspell",
    "structcheck",
    "unconvert",
    "unparam",
    "varcheck",
    "vet",
    "vetshadow"
  ],
  "Exclude": [
    "unexported",
    "error return",
    "Errors unhandled"
  ],
  "Skip": [
    "x/http"
  ]
}
